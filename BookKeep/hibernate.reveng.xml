<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="T_User" catalog="BookKeeping" class="Model.TUser">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="T_Bill" catalog="BookKeeping" class="Model.TBill">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="T_DictionaryInfo" catalog="BookKeeping"
		class="Model.TDictionaryInfo">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="T_DictionaryType" catalog="BookKeeping"
		class="Model.TDictionaryType">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="V_Bill" catalog="BookKeeping" class="Model.VBill">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>